<p id="notice"><%= notice %></p>

<h1>Webapps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Element</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webapps.each do |webapp| %>
      <tr>
        <td><%= webapp.name %></td>
        <td><%= webapp.element %></td>
        <td><%= link_to 'Show', webapp %></td>
        <td><%= link_to 'Edit', edit_webapp_path(webapp) %></td>
        <td><%= link_to 'Destroy', webapp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Webapp', new_webapp_path %>
